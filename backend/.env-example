# Configurações da rede zkVerify Testnet
ETHEREUM_RPC_URL=https://zkverify-testnet.subscan.io/
CHAIN_ID=14000

# Configurações da SubWallet
SUBWALLET_ENDPOINT=seu_endpoint_subwallet_aqui
SUBWALLET_TOKEN=seu_token_subwallet_aqui

# Chave privada para transações (NUNCA compartilhe em produção)
PRIVATE_KEY=sua_chave_privada_aqui

# Endereço do contrato
PROOF_REGISTRY_ADDRESS=seu_endereco_do_contrato_aqui

# Configurações de ambiente
NODE_ENV=development
PORT=3003

# Configurações CORS
CORS_ORIGIN=http://localhost:3000 